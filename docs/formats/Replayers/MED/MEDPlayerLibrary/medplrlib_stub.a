	JSR	-$42(A6)
	MOVEA.L	(SP)+,A6
	RTS

_LoadModule:
	MOVEA.L	4(SP),A0
	MOVE.L	A6,-(SP)
	MOVEA.L	_MEDPlayerBase,A6
	JSR	-$48(A6)
	MOVEA.L	(SP)+,A6
	RTS

_UnLoadModule:
	MOVEA.L	4(SP),A0
	MOVE.L	A6,-(SP)
	MOVEA.L _MEDPlayerBase,A6
	JSR	-$4E(A6)
	MOVEA.L	(SP)+,A6
	RTS

_GetCurrentModule:
	MOVE.L	A6,-(SP)
