	MOVE.L	A6,-(SP)
	MOVEA.L	_MEDPlayerBase,A6
	JSR	-$2A(A6)
	MOVEA.L	(SP)+,A6
	RTS

_ContModule:
	MOVEA.L	4(SP),A0
	MOVE.L	A6,-(SP)
	MOVEA.L	_MEDPlayerBase,A6
	JSR	-$30(A6)
	MOVEA.L	(SP)+,A6
	RTS

_StopPlayer:
	MOVE.L	A6,-(SP)
	MOVEA.L	_MEDPlayerBase,A6
	JSR	-$36(A6)
	MOVEA.L	(SP)+,A6
	RTS

_DimOffPlayer:
