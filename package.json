{
  "name": "devilbox",
  "private": true,
  "version": "1.190.0",
  "description": "DEViLBOX - TB-303 Acid Tracker with Devil Fish Mod",
  "repository": {
    "type": "git",
    "url": "https://github.com/spot/DEViLBOX.git"
  },
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "prebuild": "node scripts/increment-minor-version.cjs && node scripts/generate-changelog.cjs",
    "predev": "node scripts/generate-changelog.cjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "changelog": "node scripts/generate-changelog.cjs",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder build --publish never",
    "electron:build:all": "npm run build && electron-builder build -wml --publish never"
  },
  "build": {
    "appId": "com.devilbox.app",
    "productName": "DEViLBOX",
    "directories": {
      "output": "dist_electron"
    },
    "publish": {
      "provider": "github",
      "owner": "spot",
      "repo": "DEViLBOX"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.music",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "tar.gz"
      ],
      "category": "Audio"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@p5-wrapper/react": "^5.0.2",
    "@tanstack/react-virtual": "^3.13.18",
    "chiptune3": "^0.8.4",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "immer": "^11.1.3",
    "lucide-react": "^0.562.0",
    "p5": "^2.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tone": "^14.7.39",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.10.9",
    "@types/p5": "^1.7.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
